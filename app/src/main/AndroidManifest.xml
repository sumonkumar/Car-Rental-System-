<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.car_rental_system">

    <application
        android:allowBackup="true"
        android:icon="@drawable/carlogo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Car_Rental_System">
        <activity android:name=".User_ShowCar"></activity>
        <activity android:name=".Rent" />
        <activity android:name=".Admin_ShowCar" />
        <activity android:name=".Admin_AddCar" />
        <activity android:name=".Admin_Login" />
        <activity android:name=".Admin_SignUp" />
        <activity android:name=".Admin_Dashboard" />
        <activity android:name=".FirstPage" />
        <activity android:name=".Dashboard" />
        <activity android:name=".UserProfile" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>